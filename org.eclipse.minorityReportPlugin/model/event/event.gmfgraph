<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition"
    name="model">
  <figures
      name="nodes">
    <descriptors
        name="PrimitiveEventFigure">
      <actualFigure
          xsi:type="gmfgraph:RoundedRectangle"
          name="PrimitiveEvent"
          cornerWidth="30"
          cornerHeight="20">
        <foregroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="orange"/>
        <children
            xsi:type="gmfgraph:Label"
            name="PrimitiveEventName"
            text="---Primitive event name---"/>
      </actualFigure>
      <accessors
          figure="//@figures.0/@descriptors.0/@actualFigure/@children.0"/>
    </descriptors>
    <descriptors
        name="AddParamFigure">
      <actualFigure
          xsi:type="gmfgraph:Rectangle"
          name="AddParam"/>
    </descriptors>
  </figures>
  <figures
      name="svg">
    <descriptors
        name="AgentFigure">
      <actualFigure
          xsi:type="gmfgraph:CustomFigure"
          name="Agent"
          qualifiedClassName="org.eclipse.gmf.runtime.draw2d.ui.render.figures.ScalableImageFigure"/>
    </descriptors>
    <descriptors
        name="ObserverFigure">
      <actualFigure
          xsi:type="gmfgraph:CustomFigure"
          name="Observer"
          qualifiedClassName="org.eclipse.gmf.runtime.draw2d.ui.render.figures.ScalableImageFigure"/>
    </descriptors>
  </figures>
  <figures
      name="links">
    <figures
        xsi:type="gmfgraph:PolylineDecoration"
        name="PEAgentDeoration"/>
    <figures
        xsi:type="gmfgraph:PolylineDecoration"
        name="PEObserverDecoration"/>
    <figures
        xsi:type="gmfgraph:PolylineDecoration"
        name="PEAddParamDecoration"/>
    <descriptors
        name="PEAgentFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="PEAgent"
          targetDecoration="//@figures.2/@figures.0"/>
    </descriptors>
    <descriptors
        name="PEObserverFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="PEObserver"
          targetDecoration="//@figures.2/@figures.1"/>
    </descriptors>
    <descriptors
        name="PEAddParamFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="PEAddParam"
          targetDecoration="//@figures.2/@figures.2"/>
    </descriptors>
  </figures>
  <figures
      name="labels">
    <descriptors
        name="AddParamNameFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="AddParamName"
          text="NAME"/>
      <accessors
          figure="//@figures.3/@descriptors.0/@actualFigure"/>
    </descriptors>
    <descriptors
        name="AgentNameFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="AgentName"
          text="NAME"/>
      <accessors
          figure="//@figures.3/@descriptors.1/@actualFigure"/>
    </descriptors>
  </figures>
  <nodes
      name="PrimitiveEventNode"
      figure="PrimitiveEventFigure"/>
  <nodes
      name="ObserverNode"
      figure="ObserverFigure"/>
  <nodes
      name="AddParamNode"
      figure="AddParamFigure"/>
  <nodes
      name="AgentNode"
      figure="AgentFigure"/>
  <connections
      name="PEAddParamConnection"
      figure="PEAddParamFigure"/>
  <connections
      name="PEAgentConnection"
      figure="PEAgentFigure"/>
  <connections
      name="PEObserverConnection"
      figure="PEObserverFigure"/>
  <labels
      name="AgentLabel"
      figure="AgentNameFigure"/>
  <labels
      name="PrimitiveEventLabel"
      figure="PrimitiveEventFigure"
      accessor="//@figures.0/@descriptors.0/@accessors.0"/>
  <labels
      name="AddParamLabel"
      figure="AddParamNameFigure"
      elementIcon="false"/>
</gmfgraph:Canvas>
